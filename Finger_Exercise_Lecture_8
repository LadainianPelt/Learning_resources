def same_chars(s1, s2):
    """
    s1 and s2 are strings
    Returns boolean True is a character in s1 is also in s2, and vice 
    versa. If a character only exists in one of s1 or s2, returns False.
    """
s1 = str(5)
s2 = str(4)
def same_chars(s1, s2):
    for i in s1:
        if i not in s2:
            return False
    for i in s2:
        if i not in s1:
            return False
    return True
print(False or True)    